

// User function Template for Java

class Solution {
    int MissingNumber(int[] arr, int n) {
        // Your Code Here
        int sum=0, arrSum=0;
       sum=(n*(n+1))/2;
       for(int i=0; i<n-1; i++){
           arrSum=arrSum+arr[i];
       }
       return (sum-arrSum);
    }
}
